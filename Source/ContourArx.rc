IDR_MENU_POPUP MENU DISCARDABLE 
BEGIN
    POPUP "&Contour Input"
    BEGIN
        POPUP "Import Points"
        BEGIN
            MENUITEM "From &Data File...",          ID_BUTTON_IMPORTDATA
            MENUITEM "From &Current Drawing",       ID_BUTTON_ADD_POINTS
        END
        POPUP "Generate Points"
        BEGIN
            MENUITEM "&Linear Interpolation between Two points", 
                                                    ID_BUTTON_GEN_POINTS
            MENUITEM "Linear &Interpolation (From 3DPolyline Vertexes)...", 
                                                    ID_BUTTON_GEN_POINTS_MULTI_SEG

            MENUITEM SEPARATOR
            MENUITEM "&Hard Break Line between Two Points...", 
                                                    ID_BUTTON_HARD_BREAK
            MENUITEM SEPARATOR
            MENUITEM "Along Grid...",               ID_BUTTON_GEN_GRID_POINTS

        END
        MENUITEM SEPARATOR
        MENUITEM "&Check Duplicate Points",     ID_BUTTON_CHK_CONT_POINTS
    END
    POPUP "Contour"
    BEGIN
        MENUITEM "Contour &Parameters...",      ID_CONTOUR_PARAM
        MENUITEM "&Draw Contour",               ID_DRAWCONTOUR
        MENUITEM "&Draw Contour of Specified Elevation", 
                                                ID_DRAWCONTOUR_USER_SPEC
        MENUITEM SEPARATOR
        MENUITEM "&Delete All Contours...",     ID_DEL_CONTOUR
        MENUITEM SEPARATOR
        MENUITEM "&Compact Contour Lines",      ID_COMPACT_CONTOUR_LINES
        MENUITEM SEPARATOR
        MENUITEM "Calculate Volume...",         ID_CALCULATE_VOLUME
    END
    POPUP "Contour Annotation"
    BEGIN
        MENUITEM "&Annotate All Contour Lines...", ID_BUTTON_AUTO_ANN
        MENUITEM "Set Auto-Annotation &Parameters...", ID_CONTOUR_PARAM
        MENUITEM SEPARATOR
        MENUITEM "Annotate &Selected Contour Lines", ID_BUTTON_MANUAL_ANN
        MENUITEM "&Instant Annotation for Single Contour Line", 
                                                ID_BUTTON_INSTANT_ANN
        MENUITEM "&Set Annotation Parameters...", ID_SET_CONT_ANN_PARAMS
        MENUITEM SEPARATOR
        MENUITEM "&Convert Polyline to Contour Entity", 
                                                ID_BUTTON_CONVERT_TOCONTOUR_ENT

    END
    POPUP "Profile"
    BEGIN
        MENUITEM "Draw Profile...",             ID_BUTTON_PROFILE
        MENUITEM "&Edit Profile Annotation...", ID_BUTTON_PROFILE_EDIT
        MENUITEM SEPARATOR
        POPUP "Alignment"
        BEGIN
            POPUP "&Horizontal"
            BEGIN
                MENUITEM "&Create",                     ID_BUTTON_HALIGNMENT
                MENUITEM "&Edit...",                    ID_BUTTON_EDIT_HALIGNMENT

                MENUITEM "&Revert",                     ID_BUTTON_REVERT_HALIGNMENT

                MENUITEM SEPARATOR
                MENUITEM "Annotate &Vertex",            ID_BUTTON_ANN_HALIGNMENT

            END
        END
    END
    POPUP "Cross Section"
    BEGIN
        MENUITEM "&Draw Cross Section",         ID_DRAW_X_SECTION
        MENUITEM SEPARATOR
        POPUP "Alignment"
        BEGIN
            POPUP "&Horizontal"
            BEGIN
                MENUITEM "&Create",                     ID_BUTTON_HALIGNMENT
                MENUITEM "&Edit...",                    ID_BUTTON_EDIT_HALIGNMENT

                MENUITEM "&Revert",                     ID_BUTTON_REVERT_HALIGNMENT

                MENUITEM SEPARATOR
                MENUITEM "Annotate &Vertex",            ID_BUTTON_ANN_HALIGNMENT

            END
            POPUP "&Vertical"
            BEGIN
                MENUITEM "&Create",                     ID_BUTTON_VALIGNMENT
                MENUITEM "&Edit...",                    ID_BUTTON_EDIT_VALIGNMENT

                MENUITEM SEPARATOR
            END
        END
    END
    POPUP "Utilities"
    BEGIN
        MENUITEM "&Divide Polyline",            ID_DIVIDE_POLYLINE
        MENUITEM "&Revert Polyline",            ID_REVERT_POLYLINE
        MENUITEM SEPARATOR
